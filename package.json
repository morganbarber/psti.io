{
    "name": "pastebin",
    "version": "1.0.0",
    "private": true,
    "description": "Enterprise-level security-focused pastebin for hackers",
    "scripts": {
        "dev": "turbo run dev",
        "build": "turbo run build",
        "lint": "turbo run lint",
        "test": "turbo run test",
        "clean": "turbo run clean && rm -rf node_modules",
        "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
        "deploy": "sst deploy"
    },
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "devDependencies": {
        "@types/node": "^24.10.1",
        "prettier": "^3.7.3",
        "sst": "^3.17.23",
        "turbo": "^2.3.3",
        "typescript": "5.9.3"
    },
    "engines": {
        "node": ">=20.0.0",
        "npm": ">=10.0.0"
    },
    "packageManager": "npm@10.9.2"
}